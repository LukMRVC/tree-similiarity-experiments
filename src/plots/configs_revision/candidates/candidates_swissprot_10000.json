{
    "title": "Verification Candidates",
    "print_title": "no",
    "legend": "upper left",
    "legend_font_size": 18,
    "legend_frame_alpha": 0.8,
    "grid": "on",
    "dataset_name": "Swissprot 10000",
    "markers": ["s", "*", "o", "^", "p", "d", "P"],
    "markersize": [15, 20, 15, 15, 15, 15, 15],
    "markerfacecolor": "none",
    "colors": ["red", "green", "blue", "cyan", "magenta", "orange", "greenyellow"],
    "tables": [
        {
            "table_name": "tang_join", 
            "attributes": [
                {
                    "attr_name": "avg(verification_candidates)"
                }
            ],
            "name": "$\\mathtt{Tang}$",
            "constraints": {
                "upperbound": ["APTED"]
            }
        },
        {
            "table_name": "t_join", 
            "attributes": [
                {
                    "attr_name": "avg(verification_candidates - upperbound_pruned)"
                }
            ],
            "name": "$\\mathtt{TJoin}$",
            "constraints": {
                "verification_algorithm": ["Touzet"],
                "upperbound": ["greedy"]
            }
        },
        {
            "table_name": "binary_branches_join", 
            "attributes": [
                {
                    "attr_name": "avg(verification_candidates)"
                }
            ],
            "name": "$\\mathtt{BinBranches}$",
            "constraints": {
                "upperbound": ["APTED"]
            }
        },
        {
            "table_name": "histogram_join", 
            "attributes": [
                {
                    "attr_name": "avg(verification_candidates)"
                }
            ],
            "name": "$\\mathtt{Histogram}$",
            "constraints": {
                "upperbound": ["APTED"]
            }
        },
        {
            "table_name": "guha_join", 
            "attributes": [
                {
                    "attr_name": "avg(ted_verification_candidates)"
                }
            ],
            "name": "$\\mathtt{Guha\\ K=2}$",
            "constraints": {
                "reference_set_size": [2],
                "upperbound": ["APTED"]
            }
        },
        {
            "table_name": "guha_join", 
            "attributes": [
                {
                    "attr_name": "avg(ted_verification_candidates)"
                }
            ],
            "name": "$\\mathtt{Guha\\ K=4}$",
            "constraints": {
                "reference_set_size": [4],
                "upperbound": ["APTED"]
            }
        },
        {
            "table_name": "t_join", 
            "attributes": [
                {
                    "attr_name": "avg(join_result_size)"
                }
            ],
            "name": "$\\mathtt{Result}$"
        }
    ],
    "constraints": {
        "dataset_filename": ["swissprot_10000_sorted.bracket"]
    },
    "x_axis": {
        "db_column": "threshold",
        "name": "Threshold",
        "xticks": 10,
        "xmin": 0,
        "xmax": 60.001, 
        "font_size": 20, 
        "ticks_font_size": 20
    },
    "y_axis": {
        "name": "Candidates (Tree Pairs)",
        "scale": "log",
        "font_size": 20, 
        "ticks_font_size": 20, 
        "suffix": "yes"
    }
}
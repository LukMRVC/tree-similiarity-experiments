{
    "title": "Verification Candidates",
    "print_title": "no",
    "legend_font_size": 18,
    "legend_frame_alpha": 0.8,
    "legend": "lower right",
    "grid": "on",
    "dataset_name": "Sentiment",
    "markers": ["s", "*", "o", "^", "p", "d", "8"],
    "markersize": [20, 15, 15, 15, 15, 15, 15],
    "markerfacecolor": "none",
    "colors": ["green", "red", "blue", "cyan", "magenta", "orange", "greenyellow"],
    "tables": [
        {
            "table_name": "t_join",
            "attributes": [
                {
                    "attr_name": "avg(verification_candidates - upperbound_pruned)"
                }
            ],
            "name": "$\\mathtt{TJoin}$",
            "constraints": {
                "verification_algorithm": ["Touzet"],
                "upperbound": ["greedy"]
            }
        },
        {
            "table_name": "guha_rsc_join",
            "attributes": [
                {
                    "attr_name": "avg(sed_candidates - cted_result_pairs)"
                }
            ],
            "name": "$\\mathtt{Guha RSC\\ K=2}$",
            "constraints": {
                "verification_algorithm": ["APTED"],
                "reference_set_size": [2]
            }
        },
        {
            "table_name": "guha_rsc_join",
            "attributes": [
                {
                    "attr_name": "avg(sed_candidates - cted_result_pairs)"
                }
            ],
            "name": "$\\mathtt{Guha RSC\\ K=4}$",
            "constraints": {
                "verification_algorithm": ["APTED"],
                "reference_set_size": [4]
            }
        },
        {
            "table_name": "guha_rsc_join",
            "attributes": [
                {
                    "attr_name": "avg(sed_candidates - cted_result_pairs)"
                }
            ],
            "name": "$\\mathtt{Guha RSC\\ K=8}$",
            "constraints": {
                "verification_algorithm": ["APTED"],
                "reference_set_size": [8]
            }
        },
        {
            "table_name": "guha_rsc_join",
            "attributes": [
                {
                    "attr_name": "avg(sed_candidates - cted_result_pairs)"
                }
            ],
            "name": "$\\mathtt{Guha RSC\\ K=16}$",
            "constraints": {
                "verification_algorithm": ["APTED"],
                "reference_set_size": [16]
            }
        },
        {
            "table_name": "guha_rsc_join",
            "attributes": [
                {
                    "attr_name": "avg(sed_candidates - cted_result_pairs)"
                }
            ],
            "name": "$\\mathtt{Guha RSC\\ K=32}$",
            "constraints": {
                "verification_algorithm": ["APTED"],
                "reference_set_size": [32]
            }
        }
    ],
    "constraints": {
        "dataset_filename": ["sentiment_sorted.bracket"]
    },
    "x_axis": {
        "db_column": "threshold",
        "name": "Threshold",
        "xticks": 2,
        "xmin": 1,
        "xmax": 15.001, 
        "font_size": 20, 
        "ticks_font_size": 20
    },
    "y_axis": {
        "name": "Candidates (Tree Pairs)",
        "scale": "log",
        "font_size": 20, 
        "ticks_font_size": 20, 
        "suffix": "yes"
    }
}

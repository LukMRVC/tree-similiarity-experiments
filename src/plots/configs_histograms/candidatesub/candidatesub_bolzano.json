{
    "title": "False Positive Ratio",
    "print_title": "no",
    "legend": "upper left",
    "legend_font_size": 18,
    "grid": "on",
    "dataset_name": "Bolzano",
    "markers": ["s", "*", "p", "P"],
    "markersize": [15, 20, 15, 15],
    "markerfacecolor": "none",
    "colors": ["red", "green", "magenta", "greenyellow"],
    "tables": [
        {
            "table_name": "t_join", 
            "attributes": [
                {
                    "attr_name": "avg(NULLIF(verification_candidates::decimal,0) - NULLIF(upperbound_pruned::decimal,0))"
                }
            ],
            "name": "$\\mathtt{TJoin}+UB$",
            "constraints": {
                "upperbound": "greedy"
            }
        },
        {
            "table_name": "t_join", 
            "attributes": [
                {
                    "attr_name": "avg(NULLIF(verification_candidates::decimal,0))"
                }
            ],
            "name": "$\\mathtt{TJoin}$",
            "constraints": {
                "upperbound": "greedy"
            }
        },
        {
            "table_name": "label_histogram_join", 
            "attributes": [
                {
                    "attr_name": "avg(NULLIF(verification_candidates::decimal,0))"
                }
            ],
            "name": "$\\mathtt{LabelHisto}$"
        },
        {
            "table_name": "t_join", 
            "attributes": [
                {
                    "attr_name": "avg(NULLIF(join_result_size::decimal,0))"
                }
            ],
            "name": "$\\mathtt{Result}$"
        }
    ],
    "constraints": {
        "dataset_filename": "bolzano_sorted.bracket"
    },
    "x_axis": {
        "db_column": "threshold",
        "name": "Threshold",
        "xticks": 2,
        "xmin": 1,
        "xmax": 15.001, 
        "font_size": 20, 
        "ticks_font_size": 20
    },
    "y_axis": {
        "name": "Candidates (Tree Pairs)",
        "font_size": 20, 
        "ticks_font_size": 20, 
        "suffix": "yes"
    }
}
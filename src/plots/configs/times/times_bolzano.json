{
    "title": "Times",
    "print_title": "no",
    "legend_font_size": 18,
    "legend_frame_alpha": 0.8,
    "legend": "upper left",
    "grid": "on",
    "dataset_name": "Bolzano",
    "markers": ["^", "s", "D", "*", "x"],
    "markersize": [15, 15, 15, 20, 15],
    "markerfacecolor": "none",
    "colors": ["red", "orangered", "darkorange", "green", "limegreen"],
    "tables": [ 
        {
            "table_name": "tang_join", 
            "attributes": [
                {
                    "attr_name": "avg(filter_time + verification_time)/1000", 
                    "name": " - Join time"
                }
            ],
            "name": "$\\mathtt{Tang}$",
            "constraints": {
                "verification_algorithm": "APTED"
            }
        },
        {
            "table_name": "tang_join", 
            "attributes": [
                {
                    "attr_name": "avg(filter_time + verification_time)/1000", 
                    "name": " - Join time"
                }
            ],
            "name": "$\\mathtt{Tang}$+BSM",
            "constraints": {
                "verification_algorithm": "Touzet"
            }
        },
        {
            "table_name": "tang_join", 
            "attributes": [
                {
                    "attr_name": "avg(filter_time)/1000", 
                    "name": " - Cand. time"
                }
            ],
            "name": "$\\mathtt{Tang}$",
            "constraints": {
                "verification_algorithm": "Touzet"
            }
        },
        {
            "table_name": "t_join", 
            "attributes": [
                {
                    "attr_name": "avg(tree_to_set_time + filter_time + upperbound_time + verification_time)/1000", 
                    "name": " - Join time"
                },
                {
                    "attr_name": "avg(filter_time)/1000", 
                    "name": " - Cand. time"
                }
            ],
            "name": "$\\mathtt{TJoin}$",
            "constraints": {
                "verification_algorithm": "Touzet",
                "upperbound": "greedy"
            }
        }
    ],
    "constraints": {
        "dataset_filename": "bolzano_sorted.bracket"
    },
    "x_axis": {
        "db_column": "threshold",
        "name": "Threshold",
        "xticks": 2,
        "xmin": 1,
        "xmax": 15.001, 
        "font_size": 20, 
        "ticks_font_size": 20
    },
    "y_axis": {
        "name": "Time [s]",
        "scale": "log",
        "ymin": 0, 
        "font_size": 20, 
        "ticks_font_size": 20
    }
}
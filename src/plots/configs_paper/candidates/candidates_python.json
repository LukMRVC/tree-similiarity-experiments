{
    "title": "Verification Candidates",
    "print_title": "no",
    "legend": "lower right",
    "legend_font_size": 18,
    "legend_frame_alpha": 0.8,
    "grid": "on",
    "dataset_name": "Python",
    "markers": ["s", "*", "o", "^", "p", "d", "8"],
    "markersize": [15, 20, 15, 15, 15, 15, 15],
    "markerfacecolor": "none",
    "colors": ["red", "green", "blue", "cyan", "magenta", "orange", "greenyellow"],
    "tables": [
        {
            "table_name": "tang_join", 
            "attributes": [
                {
                    "attr_name": "avg(verification_candidates)"
                }
            ],
            "name": "$\\mathtt{Tang}$",
            "constraints": {
                "verification_algorithm": ["APTED"]
            }
        },
        {
            "table_name": "t_join", 
            "attributes": [
                {
                    "attr_name": "avg(verification_candidates)"
                }
            ],
            "name": "$\\mathtt{TJoin}$",
            "constraints": {
                "verification_algorithm": ["Touzet"],
                "upperbound": ["greedy"]
            }
        },
        {
            "table_name": "binary_branches_join", 
            "attributes": [
                {
                    "attr_name": "avg(verification_candidates)"
                }
            ],
            "name": "$\\mathtt{Binary}$",
            "constraints": {
                "verification_algorithm": ["APTED"]
            }
        },
        {
            "table_name": "histogram_join", 
            "attributes": [
                {
                    "attr_name": "avg(verification_candidates)"
                }
            ],
            "name": "$\\mathtt{Histo}$",
            "constraints": {
                "verification_algorithm": ["APTED"]
            }
        },
        {
            "table_name": "guha_join", 
            "attributes": [
                {
                    "attr_name": "avg(l_t_candidates - u_t_result_pairs)"
                }
            ],
            "name": "$\\mathtt{Guha}$",
            "constraints": {
                "reference_set_size": [8],
                "verification_algorithm": ["APTED"]
            }
        },
        {
            "table_name": "t_join", 
            "attributes": [
                {
                    "attr_name": "avg(verification_candidates - upperbound_pruned)"
                }
            ],
            "name": "$\\mathtt{TJoin\\ VC}$",
            "constraints": {
                "verification_algorithm": ["Touzet"],
                "upperbound": ["greedy"]
            }
        },
        {
            "table_name": "t_join", 
            "attributes": [
                {
                    "attr_name": "avg(join_result_size)"
                }
            ],
            "name": "$\\mathtt{Result}$"
        }
    ],
    "constraints": {
        "dataset_filename": ["python_sorted.bracket"]
    },
    "x_axis": {
        "db_column": "threshold",
        "name": "Threshold",
        "xticks": 2,
        "xmin": 1,
        "xmax": 20.001, 
        "font_size": 20, 
        "ticks_font_size": 20
    },
    "y_axis": {
        "name": "Candidates (Tree Pairs)",
        "scale": "log",
        "font_size": 20, 
        "ticks_font_size": 20
    }
}
